<div class="row dataRow">
  <div class="span3">
    <%= visit.visittime %>
  </div>
  <div class="span3 ">
    <strong><%= visit.chiefComplaint %></strong>
  </div>
  <div class="span2">
    <strong><%= visit.dischargeDiagnosis %></strong>
  </div>
  <div class="span3">
    <% if visit.id == @selectedVisit.id %>
    <strong>[Selected]</strong>
    <% else %>
  	<%= link_to 'select', patient_path(:selectedVisit => visit) %>
    <% end %>
  </div>
</div>