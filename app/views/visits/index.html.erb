<h1>Listing visits</h1>

<table>
  <tr>
    <th>Patient</th>
    <th>Mrnumber</th>
    <th>Chiefcomplaint</th>
    <th>Visitsubject</th>
    <th>Isactivesim</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @visits.each do |visit| %>
  <tr>
    <td><%= visit.patient_id %></td>
    <td><%= visit.mrNumber %></td>
    <td><%= visit.chiefComplaint %></td>
    <td><%= visit.visitSubject %></td>
    <td><%= visit.isActiveSim %></td>
    <td><%= link_to 'Show', visit %></td>
    <td><%= link_to 'Edit', edit_visit_path(visit) %></td>
    <td><%= link_to 'Destroy', visit, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Visit', new_visit_path %>
