<div class="patient-info-header" >
	<h3>Procedure and Medical Administration Log </h3>
</div>
<div id="mar" class="patientInfo"> 	
<%if @medical_administration_records.any? %>
	<div class="row-fluid">
	  <%= render @medical_administration_records %>
	</div>
	<% else %>
	  <div class="message">
	  	<span class="label label-info">
	  		Note:
	 		</span> 
	  	<div> 
	  	  No medical administrations or procedures are associated with this vist.</br>
			</div>
	  </div>
	<%end%>
	<div class="btn bottomButton">
		<%= link_to 'New MAR entry', new_medical_administration_record_path(:visit_id => @selectedVisit.id), remote: true%>
	</div>
</div>