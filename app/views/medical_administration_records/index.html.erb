<h1>Listing medical_administration_records</h1>

<table>
  <tr>
    <th>Visit</th>
    <th>Time recorded</th>
    <th>Action description</th>
    <th>Action details</th>
    <th>Clinician signature</th>

    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @medical_administration_records.each do |medical_administration_record| %>
  <tr>
    <td><%= medical_administration_record.visit_id %></td>
    <td><%= medical_administration_record.time_recorded %></td>
    <td><%= medical_administration_record.description %></td>
    <td><%= medical_administration_record.note %></td>
    <td><%= medical_administration_record.clinician_signature %></td>

    <td><%= link_to 'Show', medical_administration_record %></td>
    <td><%= link_to 'Edit', edit_medical_administration_record_path(medical_administration_record) %></td>
    <td><%= link_to 'Destroy', medical_administration_record, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

